#!/usr/bin/env node
var elfu = require('elfu')
var name = process.argv[2]
if (name == '-v' || name == '--version') {
	console.log(elfu.version)
	return
}

var fs = require('fs')
if (name) {
	var s = fs.readFileSync(name).toString()
	s = elfu.convert(s, name)
	process.stdout.write(s)
} else {
	console.log('node yyj <in>')
}


