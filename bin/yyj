#!/usr/bin/env node
var elfu = require('elfu')
if (name == '-v' || name == '--version') {
	console.log(elfu.version)
	return
}

var fs = require('fs')
var fin = process.argv[2]
if (fin) {
	var s = fs.readFileSync(fin).toString()
	s = elfu.convert(s, fin)
	process.stdout.write(s)
} else {
	console.log('node yyj <in>')
}


